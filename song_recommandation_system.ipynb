    "def clean_features(value):\n",
    "    value = re.sub(r'[{}()\"]', '', value)\n",
    "    value = value.encode('utf-8').decode('utf-8', errors='ignore')\n",
    "    if not value:\n",
    "        return None\n",
    "    return [v.strip().replace(\" \", \"\") for v in value.split(',')]"
   ]
